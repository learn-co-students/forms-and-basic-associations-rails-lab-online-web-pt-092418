<h1>Create a Song</h1>

<%= form_for :song, url:songs_path do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
<%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
    <label>Notes 1</label><input name='song[note_contents][]' id="song_notes_1">
    <label>Notes 2</label><input name='song[note_contents][]' id="song_notes_2">
  <%= f.submit %>
<% end %>


